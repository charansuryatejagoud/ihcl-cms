  "scripts": {
    "login": "sanity login",
    "start": "sanity start",
    "build": "sanity build ./build --yes",
    "delete-unused-assets": "ts-node --transpile-only scripts/delete-unused-images.ts",
    "migrate-cdp": "ts-node --transpile-only scripts/migrate-cdp/index.ts",
    "populate-metadata-identifier": "ts-node --transpile-only scripts/populate-metadata-identifier/index.ts",
    "convert-to-sanity": "MODEL_FILE=\"$HOME/Downloads/en.model.json\" ts-node --transpile-only scripts/aem-to-sanity/index.ts",
    "check-urls": "MODEL_FILE=\\\"$HOME/Downloads/en.model.json\\\" ts-node --transpile-only scripts/export-excel.ts",
    "upload-to-sanity": "sanity dataset import \"$HOME/Downloads/pages.ndjson\" develop --replace --allow-failing-assets",
    "excel-export": "deno run --allow-all scripts/export-to-excel/index.ts",
    "build-export": "deno compile --allow-all --output dist/sanity-export scripts/export-to-excel/index.ts && cp scripts/export-to-excel/README.md dist/"
  },
  "keywords": [
    "sanity"
  ],
